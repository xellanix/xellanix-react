{
	"name": "xellanix-react",
	"version": "2.2.4",
	"description": "A react stylesheet and components from xellanix",
	"main": "dist/xellanix.umd.js",
	"module": "dist/xellanix.es.js",
	"types": "dist/types/index.d.ts",
	"scripts": {
		"build": "tsc -b && vite build"
	},
	"devDependencies": {
		"@types/node": "^22.0.0",
		"@types/react": "^18.3.3",
		"@vitejs/plugin-react": "^4.3.1",
		"typescript": "^5.5.4",
		"vite": "^5.3.4",
		"vite-plugin-dts": "4.0.0-beta.1"
	},
	"exports": {
		".": {
			"import": "./dist/xellanix.es.js",
			"require": "./dist/xellanix.umd.js",
			"types": "./dist/types/index.d.ts"
		},
		"./style.css": "./dist/assets/style.css"
	},
	"files": [
		"dist",
		"package.json",
		"README.md"
	],
	"keywords": [
		"react",
		"xellanix",
		"stylesheets",
		"components"
	],
	"author": "xellanix",
	"license": "MIT",
	"homepage": "https://xellanix.github.io/xellanix-react-docs/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xellanix/xellanix-react.git"
	}
}
